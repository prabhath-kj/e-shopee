<script>
  function handleChange(event) {
    const status = event.target.value;
    const url = `/admin/view-user?status=${status}`;
    window.location.href = url;
  }

  $(document).ready(function () {
    $("#myTable").DataTable();
  });

  function cropImage() {
  // Get the Cropper instance for each input field
  var cropper1 = $("#image1").cropper("getCropperData");
  var cropper2 = $("#image2").cropper("getCropperData");
  var cropper3 = $("#image3").cropper("getCropperData");
  var cropper4 = $("#image4").cropper("getCropperData");

  // Get the cropped image data for each input field as a base64-encoded JPEG image
  var croppedImageDataURL1 = $("#image1").cropper("getCroppedCanvas").toDataURL("image/jpeg");
  var croppedImageDataURL2 = $("#image2").cropper("getCroppedCanvas").toDataURL("image/jpeg");
  var croppedImageDataURL3 = $("#image3").cropper("getCroppedCanvas").toDataURL("image/jpeg");
  var croppedImageDataURL4 = $("#image4").cropper("getCroppedCanvas").toDataURL("image/jpeg");

  // Update the input fields with the cropped image data
  $("#image1").val(croppedImageDataURL1);
  $("#image2").val(croppedImageDataURL2);
  $("#image3").val(croppedImageDataURL3);
  $("#image4").val(croppedImageDataURL4);
}


 

</script>
