  <body>
    <section class="content-main mt-80 mb-80">
      <div
        class="card mx-auto mt-auto card-login"
        style="height: 30rem; width: 30rem"
      >
        <div class="card-body">
          <h1 class="text-primary text-center mb-4">Admin Login</h1>
          <form
            name="loginForm"
            action="/admin/login"
            method="post"
            onsubmit="return validateForm()"
          >
            <div class="mb-3">
              <label for="adLogEmail">Username or Email</label>
              <input
                autocomplete="off"
                class="form-control"
                placeholder="Username or email"
                type="text"
                name="adLogEmail"
              />
              <div
                id="error-messageUserName"
                class="text-center"
                style="color: rgb(180, 17, 17)"
              ></div>
            </div>
            <!-- form-group// -->
            <div class="mb-3">
              <label for="adLogPassword">Password</label>
              <input
                class="form-control"
                placeholder="Password"
                type="password"
                name="adLogPassword"
              />
              <div
                id="error-messagePassword"
                class="text-center"
                style="color: rgb(180, 17, 17)"
              ></div>
            </div>
            <!-- form-group// -->
            <div class="mb-3">
              <label class="form-check">
                <input type="checkbox" class="form-check-input  type="radio"
                name="Remember" />
                <span class="form-check-label">Remember</span>
              </label>
            </div>
            <!-- form-group form-check .// -->
            <% if (adLogErr){%>
            <p class="text-center" style="color: rgb(180, 17, 17)">
              Please Enter Valid Details
            </p>
            <%}%>
            <div>
              <button type="submit" class="btn bg-primary w-100">
                <p class="text-white">LogIn</p>
              </button>
            </div>
            <!-- form-group// -->
          </form>
        </div>
      </div>
    </section>

    <script src="../adminassets///js/vendors/jquery-3.6.0.min.js"></script>
    <script src="../adminassets///js/vendors/bootstrap.bundle.min.js"></script>
    <script src="../adminassets///js/vendors/select2.min.js"></script>
    <script src="../adminassets///js/vendors/perfect-scrollbar.js"></script>
    <script src="../adminassets///js/vendors/jquery.fullscreen.min.js"></script>
    <script src="../adminassets///js/vendors/chart.js"></script>
    <!-- Main Script -->
    <script src="../adminassets///js/main.js" type="text/javascript"></script>
    <script
      src="../adminassets///js/custom-chart.js"
      type="text/javascript"
    ></script>
    <script>
      function validateForm() {
        var email = document.forms["loginForm"]["adLogEmail"].value;
        var password = document.forms["loginForm"]["adLogPassword"].value;
        var nameErr = document.getElementById("error-messageUserName");
        var passwordErr = document.getElementById("error-messagePassword");

        if (email == "") {
          nameErr.innerHTML = "Email must be filled out";
        } else {
          nameErr.innerHTML = "";
        }

        if (password == "") {
          passwordErr.innerHTML = "Password must be filled out";
        } else {
          passwordErr.innerHTML = "";
        }

        if (email == "" || password == "") {
          return false;
        } else {
          return true;
        }
      }
    </script>
  </body>

